{
    "expo": {
        "name": "GeoScout",
        "slug": "GeoScout",
        "privacy": "public",
        "sdkVersion": "33.0.0",
        "platforms": ["ios", "android"],
        "version": "0.5.1",
        "orientation": "portrait",
        "icon": "./assets/icon.png",
        "primaryColor": "#1abc9c",
        "splash": {
            "image": "./assets/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": ["**/*"],
        "ios": {
            "supportsTablet": true
        },
        "android": {
            "package": "com.geoscout",
            "permissions": [
                "ACCESS_COARSE_LOCATION",
                "ACCESS_FINE_LOCATION",
                "WRITE_EXTERNAL_STORAGE"
            ]
        }
    }
}
